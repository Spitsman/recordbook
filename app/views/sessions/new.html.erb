<h1>Log in </h1>

<%= form_for :user_session, url: login_path do |f| %> 
  <%= f.text_field :email, :placeholder => "Email" %> </br>  
  <%= f.password_field :password, :placeholder => "Password" %> </br>
  <%= f.submit "Log in", class: "btn-submit" %>
<% end %>

<p><%= link_to 'Sign up', signup_path %></p>